{
  "name": "Onatrix RTE",
  "alias": "Onatrix.RTE",
  "extensions": [
    {
      "type": "tiptapToolbarExtension",
      "kind": "styleMenu",
      "alias": "Onatrix.Tiptap.StyleMenu",
      "name": "Onatrix Tiptap Style Menu",
      "meta": {
        "alias": "OnatrixStyleMenu",
        "icon": "icon-palette",
        "label": "Onatrix styles"
      },
      "items": [
        {
          "label": "Headings",
          "items": [
            {
              "label": "Heading 2",
              "data": { "tag": "h2" },
              "appearance": { "icon": "icon-heading-2" }
            },
            {
              "label": "Heading 3",
              "data": { "tag": "h3" },
              "appearance": { "style": "font-size: large;" }
            },
            {
              "label": "Heading 4",
              "data": { "tag": "h4" }
            }
          ]
        },
        {
          "label": "Bullets",
          "data": {
            "tag": "ul",
            "class": "rte-ul"
          }
        },
        {
          "label": "Colors",
          "items": [
            {
              "label": "Primary",
              "data": {
                "tag": "span",
                "class": "text-primary"
              },
              "appearance": { "style": "background-color:#4F5955" }
            },
            {
              "label": "Body",
              "data": {
                "tag": "span",
                "class": "text-body-text"
              },
              "appearance": { "style": "background-color:#535656" }
            },
            {
              "label": "SubColor3",
              "data": {
                "tag": "span",
                "class": "text-subcolor-3"
              },
              "appearance": { "style": "background-color:#d9c3a9" }
            }
          ]
        }
      ]
    }

  ]
}